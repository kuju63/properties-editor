{
  "name": "properties-editor",
  "displayName": "Properties Editor",
  "description": "",
  "version": "0.1.4",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kuju63/properties-editor"
  },
  "preview": true,
  "keywords": [
    "properties",
    "editor",
    "extension"
  ],
  "icon": "images/icon.png",
  "publisher": "kuju63",
  "activationEvents": [
    "onCustomEditor:viewType:properties.editor"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "properties.editor",
        "displayName": "Properties Editor",
        "selector": [
          {
            "filenamePattern": "*.properties"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint",
    "test": "vscode-test",
    "format": "npx @biomejs/biome format --write ./src",
    "package": "vsce package",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "22.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0-alpha.42",
    "@typescript-eslint/parser": "^8.0.0-alpha.42",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^3.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.3.3"
  }
}
